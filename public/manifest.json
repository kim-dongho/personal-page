{
  "manifest_version": 3,
  "name": "Personalized Dashboard",
  "version": "1.0",
  "description": "A personalized new tab page with search, todos, shortcuts, and weather.",
  "icons": {
    "16": "favicon.ico",
    "48": "favicon.ico",
    "128": "favicon.ico"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "geolocation"
  ],
  "host_permissions": [
    "https://api.openweathermap.org/*",
    "https://www.google.com/s2/favicons*"
  ]
}